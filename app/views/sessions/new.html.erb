 <div class="container">
     <div class="row">
         <div class="col-md-6">
            <h2>Instagram, partagez vos photos avec vos amis.</h2>
            <img src="https://w7.pngwing.com/pngs/16/46/png-transparent-made-in-kings-heath-instagram-facebook-female-graphy-instagram-logo-instagram-icon-text-trademark-magenta.png" class="center-block", style="width: 300px;">
        </div>
         <div class="col-md-6"><h1> Log in </h1>
            <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
            <%= f.label :email%>
            <%= f.email_field :email, class: "form-control", id: "email", placeholder: "Enter email"%>
            <%= f.label :password%> 
            <%= f.password_field :password, class: "form-control", id: "password", placeholder: "Enter password"%> 
            <%= f.submit "Log in", class: "btn btn-success"%>
            <% end%>
        </div>
         
     </div>
 </div>

 <%= render 'blogs/footer'%>
 
 